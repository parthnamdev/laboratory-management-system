<%- include("partials/header"); -%> 
    
    <div class="custom-padding">
      <h1><%= name %></h1>
      <form action="/saveReport/<%= uuid %>" method="post" class="row">
      
        <% for(let i = 0; i < tests.length; i++){ %>
          <h2><%= tests[i].test %></h2>
          
            <% for(let j = 0; j < tests[i].entries.length; j++){ %>
              <div class="mb-3 col-lg-4 col-md-6">
                <label for="name" class="form-label"><%= tests[i].entries[j].name %></label>
                
                <%if(values.length > 0){%>
                  <input type="text" class="form-control values" id="name" name="<%= tests[i].test %>" value="<%= values[i].values[j] %>">
                <% } else {%>
                  <input type="text" class="form-control values" id="name" name="<%= tests[i].test %>" value="">
                <% }; %>
               
              </div>
            <% }; %>
            
            
      <% }; %>
      <button type="submit" class="btn btn-dark">Submit</button>
      </form>       
    </div>
</body>
